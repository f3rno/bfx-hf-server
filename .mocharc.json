{
  "ui": "bdd",
  "spec": "'test",
  "extension": "js",
  "package": "./package.json",

  "jobs": 12,
  "retries": 2,
  "parallel": true,

  "timeout": "5000",
  "slow": "1000",

  "bail": false,
  "color": true,
  "growl": true,
  "recursive": true,
  "noHighlighting": false,

  "reporter": "markdown",

  ! "reporterOptions": "",

  "diff": true,
  "inlineDiff": true,
  "fullTrace": true,
  "experimentalModules": true,
  "allowUncaught": false,
  "asyncOnly": true,
  "checkLeaks": true,
  "forbidOnly": true,
  "forbidPending": true,

  "enable-source-maps": true,

  "reporters": [
    "progress",
    "markdown",
    "landing",
    "spec",
    "list",
    "dot"
  ]
}
